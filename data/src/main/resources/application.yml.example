server:
  port: 8083
spring:
  data:
    mongodb:
      uri: mongodb://<user>:<password>@<host>:27017/<database>
  rabbitmq:
    host: localhost
    port: 5672
    username: <username>
    password: <password>
  jackson:
    serialization:
      WRITE_DATES_AS_TIMESTAMPS: false

partner:
  base-url: http://localhost:8090
  paths:
    start: /partner/encounters/start
    end: /partner/encounters/end
    lab: /partner/labs
